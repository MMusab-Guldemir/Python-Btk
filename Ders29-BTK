email = "mmusabgldmr@gmail.com"
parola = "123456"

if(email != "mmusabgldmr@gmail.com"):
    print("email yanlış")
elif(parola != "123456"):
    print("parola yanlış")
else:
    print("login olundu")



x = 30
y = 30

if(x < y):
    print("x y'den küçük")
elif(y < x):
    print("y x'den küçük")
else:
    print("x y eşit")
